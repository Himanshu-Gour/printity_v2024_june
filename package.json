    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "keywords": [
    "printity",
    "himanshu",
    "gour"
  ],
  "author": "himanshu gour",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "flash": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.22",
    "nodemailer": "^6.5.0",
    "noty": "^3.2.0-beta-deprecated",
    "passport": "^0.7.0",
